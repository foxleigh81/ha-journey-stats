{
    "name": "ha-journey-stats",
    "version": "0.0.0",
    "description": "Take the commutes that my wife and I regularly do and show stats for them.",
    "author": "Alex Foxleigh",
    "private": true,
    "scripts": {
        "dev": "PORT=3000 nuxt",
        "dev-debug": "node --inspect node_modules/.bin/nuxt",
        "build": "nuxt build",
        "start": "PORT=3000 nuxt start",
        "generate": "nuxt generate",
        "lint": "eslint --ext .js,.vue .",
        "precommit": "npm run lint",
        "test": "jest --silent",
        "test:watch": "jest --silent --watch",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "jest": {
        "verbose": false,
        "moduleFileExtensions": [
            "js",
            "vue"
        ],
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/$1"
        },
        "transform": {
            "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
            ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
        },
        "transformIgnorePatterns": [
            "<rootDir>/node_modules/(?!element-ui)"
        ],
        "snapshotSerializers": [
            "<rootDir>/node_modules/jest-serializer-vue"
        ],
        "setupFiles": [
            "<rootDir>/__tests__/setupFile.js",
            "jest-localstorage-mock"
        ],
        "testEnvironment": "jest-environment-jsdom-global",
        "testMatch": [
            "**/?(*.)(spec|test).js?(x)"
        ]
    },
    "dependencies": {
        "d3": "^5.7.0",
        "element-theme": "^2.0.1"
    },
    "devDependencies": {
        "@nuxtjs/font-awesome": "^1.0.3",
        "@nuxtjs/proxy": "^1.1.3",
        "@nuxtjs/pwa": "^2.0.8",
        "@nuxtjs/router": "^1.0.1",
        "@storybook/addon-actions": "^3.4.10",
        "@storybook/addon-links": "^3.4.10",
        "@storybook/addons": "^3.4.8",
        "@storybook/vue": "^3.4.10",
        "apollo-cache-inmemory": "^1.2.8",
        "apollo-client": "^2.4.0",
        "apollo-link": "^1.2.2",
        "apollo-link-http": "^1.5.4",
        "autoprefixer": "^7.2.3",
        "axios": "^0.17.1",
        "babel-core": "^6.26.3",
        "babel-eslint": "^7.2.3",
        "babel-helper-vue-jsx-merge-props": "^2.0.3",
        "babel-jest": "^21.2.0",
        "babel-plugin-component": "^0.10.1",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
        "babel-plugin-transform-vue-jsx": "^3.7.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "babel-preset-stage-3": "^6.24.1",
        "babel-preset-vue": "^2.0.2",
        "eslint": "^4.3.0",
        "eslint-config-standard": "^10.2.1",
        "eslint-loader": "^1.9.0",
        "eslint-plugin-html": "^3.1.1",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-node": "^5.1.1",
        "eslint-plugin-promise": "^3.5.0",
        "eslint-plugin-standard": "^3.0.1",
        "graphql": "^0.13.2",
        "graphql-tag": "^2.9.2",
        "jest": "^23.1.0",
        "jest-cli": "^23.1.0",
        "jest-environment-jsdom": "^22.0.4",
        "jest-environment-jsdom-global": "^1.0.2",
        "jest-localstorage-mock": "^2.1.0",
        "jest-serializer-vue": "^0.3.0",
        "node-sass": "^4.7.2",
        "normalize.css": "^7.0.0",
        "nuxt": "^1.4.0",
        "postcss-clean": "^1.1.0",
        "ramda": "^0.25.0",
        "sass-loader": "^6.0.6",
        "vue-apollo": "^3.0.0-beta.19",
        "vue-jest": "^1.4.0",
        "vue-server-renderer": "2.5.17",
        "vue-style-loader": "^3.0.3",
        "vue-test-utils": "^1.0.0-beta.9",
        "vuex-router-sync": "^5.0.0",
        "write-file-webpack-plugin": "^4.3.2",
        "element-theme-chalk": "^2.1.0",
        "element-ui": "^2.0.11"
    }
}
